<h1>Listing committees</h1>

<table>
  <tr>
    <th>Member</th>
    <th>Society</th>
    <th>Desgination</th>
    <th>Start date</th>
    <th>End date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @committees.each do |committee| %>
  <tr>
    <td><%= committee.member_id %></td>
    <td><%= committee.society_id %></td>
    <td><%= committee.desgination %></td>
    <td><%= committee.start_date %></td>
    <td><%= committee.end_date %></td>
    <td><%= link_to 'Show', committee %></td>
    <td><%= link_to 'Edit', edit_committee_path(committee) %></td>
    <td><%= link_to 'Destroy', committee, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Committee', new_committee_path %>
